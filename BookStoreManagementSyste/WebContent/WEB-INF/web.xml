<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>AdminLoginServlet</servlet-name>
		<servlet-class>test.AdminLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminLoginServlet</servlet-name>
		<url-pattern>/log1</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddBookServlet</servlet-name>
		<servlet-class>test.AddBookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddBookServlet</servlet-name>
		<url-pattern>/addB</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>VerifyBookServlet</servlet-name>
		<servlet-class>test.VerifyBookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VerifyBookServlet</servlet-name>
		<url-pattern>/verify</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>FinalAddBookServlet</servlet-name>
		<servlet-class>test.FinalAddBookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FinalAddBookServlet</servlet-name>
		<url-pattern>/finalB</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Index.html</welcome-file>
	</welcome-file-list>
</web-app>